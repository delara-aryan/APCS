		//following data types
		//byte		short		int 		long
		//float		double
		//char      boolean		String

		//integer variables
		byte b = 127;
		short s = -12345;
		int i = 987654;
		long l = 987654321;

		//decimal variables
		float f = 12.3456f;
		double d = 987.654;

		//other integer types
		char c = 'a';

		//other types
		String st = "Hello World!";

		//output your information here
		System.out.println("/////////////////////////////////");
		System.out.println("*Delara Aryan           02/01/18*");
		System.out.println("*                               *");
		System.out.println("*        integer types          *");
		System.out.println("*8 bit - byte = " + b + "             *");
		System.out.println("*16 bit - short = " + s + "        *");
		System.out.println("*64 bit - long = " + l + "      *");
		System.out.println("*        real types             *");
		System.out.println("*                               *");
		System.out.println("*8 bit - byteOne = "+b+"\t\t*");
