public class Lab02f
{
	public static void main( String args[] )
    {
		Line l1 = new Line(1, 2 ,3, 4);
		l1.calculateSlope();
		l1.print();
		l1.setCoordinates(3, 7, 42, 6);
		l1.calculateSlope();
		l1.print();
		l1.setCoordinates(-3, 6, -12, -6);
		l1.calculateSlope();
		l1.print();
		l1.setCoordinates(-12, 56, 3, 18);
		l1.calculateSlope();
		l1.print();
		l1.setCoordinates(5, 1, 14, 3);
		l1.calculateSlope();
		l1.print();
	}
}